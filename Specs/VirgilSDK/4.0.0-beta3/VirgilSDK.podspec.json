{
  "name": "VirgilSDK",
  "version": "4.0.0-beta3",
  "summary": "Virgil SDK for Apple devices and languages.",
  "cocoapods_version": ">= 0.36",
  "homepage": "https://github.com/VirgilSecurity/virgil-sdk-x/tree/v4",
  "license": {
    "type": "BSD",
    "file": "LICENSE"
  },
  "authors": {
    "Oleksandr Deundiak": "deundiak@gmail.com"
  },
  "platforms": {
    "ios": "7.0"
  },
  "source": {
    "git": "https://github.com/VirgilSecurity/virgil-sdk-x.git",
    "tag": "4.0.0-beta3"
  },
  "weak_frameworks": "Foundation",
  "module_name": "VirgilSDK",
  "source_files": "Classes/**/*.{h,m}",
  "public_header_files": [
    "Classes/Client/*.{h}",
    "Classes/Client/Models/*.{h}",
    "Classes/Crypto/*.{h}",
    "Classes/Crypto/Keys/*.{h}"
  ],
  "requires_arc": true,
  "dependencies": {
    "VirgilCrypto": [
      "~> 2.0"
    ]
  }
